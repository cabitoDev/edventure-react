{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "curly": ["error", "all"]
  }
}
